    "eslint-plugin-vue": "9.9.0",
    "npm-run-all": "4.1.5",
    "postcss-html": "1.5.0",
    "prettier": "2.8.4",
    "sass": "1.58.1",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-spaceninja": "11.0.0",
    "vite": "4.1.1"
