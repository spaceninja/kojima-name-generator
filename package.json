    "eslint": "8.49.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-vue": "9.17.0",
    "npm-run-all": "4.1.5",
    "postcss-html": "1.5.0",
    "prettier": "3.0.3",
    "sass": "1.66.1",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-spaceninja": "13.0.1",
    "vite": "4.4.9"
  }
