    "npm-run-all2": "8.0.4",
    "postcss-html": "1.8.1",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "stylelint": "16.26.1",
    "stylelint-config-recommended-vue": "1.6.1",
