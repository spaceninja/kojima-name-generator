{
  "name": "kojima-name-generator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "run-s lint:js lint:css lint:prettier",
