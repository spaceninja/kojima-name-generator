    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.25.0",
    "npm-run-all2": "6.1.2",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "sass": "1.75.0",
    "stylelint": "16.3.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-spaceninja": "15.0.0",
    "vite": "5.2.10"
